{
  "name": "Translate Code Monkey to Hebrew",
  "description": "ההרחבה הזאת מתרגמת את הפקודות של קוד מאנקי לעברית",
  "version": "0.2.6",
  "permissions": ["tabs", "http://*.playcodemonkey.com/*"],
      "content_scripts" : [{
	      "matches": ["https://*.playcodemonkey.com/*"],
          "js" : ["doWork.js"]
      }],
	  
  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2
}
